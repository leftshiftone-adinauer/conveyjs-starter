{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "modules": "commonjs"
        }
      }
    ]
  ],
  "plugins": [
    "styled-jsx/babel",
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "@bootstrap": "./bootstrap",
          "@components": "./components",
          "@convey": "./convey",
          "@environment": "./environment",
          "@handler": "./handler",
          "@layout": "./layout",
          "@lib": "./lib",
          "@utils": "./utils"
        }
      }
    ]
  ]
}
